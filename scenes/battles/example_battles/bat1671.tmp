script/source = "extends AttackManager

var commentaryDict = {0: \"(disable:z)(sound:mono2)* You felt your si- WAIT IS \\n THAT SANS UNDERTALE SMOKING A \\n GIGANTIC FUCKING DOOBIE!?\", 2 : \"(disable:z)(sound:mono2)* It seems like Sans is\\n getting slower with\\n every hit\", 5 : \"(disable:z)(sound:mono2)*You can now smoke in your \\n [ACT] menu!\"}
var miscCommentaries = [\"(disable:z)(sound:mono2)* You felt your lungs crawling\\n out your chest\", \"(disable:z)(sound:mono2)* If edging is a\\n skibidi chad move, is\\n jelquing beta rizz?\", \"(disable:z)(sound:mono2)* Now thats what I call undertale.\", \"(disable:z)(sound:mono2)* Smells like za\"]

func _init():
	attacks = [preload(\"res://scripts/battle/attacks/test_attack.gd\")]

func _ready():
	super._ready()

func set_writer_text():
	if(commentaryDict.has(turn_num)):
		vars.main_writer.writer_text = commentaryDict[turn_num] 
	else:
		var rng = RandomNumberGenerator.new()
		vars.main_writer.writer_text = miscCommentaries.pick_random()
 
"

[sub_resource type="GDScript" id="GDScript_vt12r"]
script/source = "extends DialougeManager

@onready var atkManager = get_parent().get_node(\"attack_manager\")
var dialogueArray = [\"Sup, kid!\",\"You know, at first I wanted to kill you.\",\"But, I started thinking...\",\"What if you lowkey chill like that?\",\"If you can outsmoke me, Ill give up.\"]
var enemy : Node2D
@onready var dirtyDict : Array = []

func start():
	enemy = vars.enemies.get_child(0)
	enemy.speech_bubble.visible = true
	enemy.get_node(\"head\").frame = 3
	enemy.get_node(\"torso\").frame = 1
	enemy.speech_writer.writer_text = \"(sound:sans)\"+dialogueArray[atkManager.turn_num]+\"(pc)\"
	await enemy.speech_writer.done
	enemy.speech_bubble.visible = false
	enemy.get_node(\"head\").frame = 0
	enemy.get_node(\"torso\").frame = 0
	await get_tree().process_frame
	done.emit()

func tfReaction():
	enemy = vars.enemies.get_child(0)
	enemy.speech_bubble.visible = true
	enemy.speech_bubble.get_node(\"speech_writer\").visible = true
	enemy.speech_writer.writer_text = \"(sound:sans)WHAT HAVE YOU DONE TO ME??(pc)\"
	await enemy.speech_writer.done
	enemy.speech_bubble.visible = false
	await get_tree().process_frame
	done.emit()

func checkDD():
	print(\"I am made of soul\")
	if(dirtyDict.has(vars.attack_manager.turn_num)):
		enemy.freeze()
		audio.pause_music()
		vars.scene.get_node(\"Videos\").queue_free()
		print(\"I am made of flesh\")
		enemy = vars.enemies.get_child(0)
		enemy.speech_bubble.visible = true
		enemy.speech_writer.writer_text = \"(sound:sans)Is this genuinely the level of humor here?(pc)\"
		await enemy.speech_writer.done
		enemy.speech_writer.writer_text = \"(sound:sans)Im outta here(pc)\"
		await enemy.speech_writer.done
		enemy.speech_bubble.visible = false
		enemy.walkAway()
		done.emit()
	else:
		await get_tree().create_timer(0.3).timeout
		done.emit()
"

