script/source = "extends Enemy

@onready var torso_position = $torso.position
@onready var head_position = $head.position
var torso_offset := Vector2.ZERO
var head_offset := Vector2.ZERO


func _init():
	super._init(\"Sans\",1,1)

func walkAway():
	isDisappointed = true
	
func freeze():
	isMoving = false

func _ready():
	sprite = $legs
	damage_anchor = $damage_anchor
	show_health_bar = false
	dodge = e_dodge.dodge

func _process(delta):
	if(isMoving == true):
		siner += 30 * delta
		match(animation_mode):
			0: torso_offset = Vector2(0, 0)
			1: torso_offset = Vector2(cos(siner / 5), sin(siner / 2.5))
			2: torso_offset.y = sin(siner / 15) * 4
			3: torso_offset.y = sin(siner / 18) * 2
		$torso.position = torso_position + Vector2(torso_offset.x, torso_offset.y / 1.5)
		$head.position = head_position + (torso_offset + head_offset)
	if(isDisappointed == true):
		position = Vector2(position.x - 1, position.y)

func check():
	vars.hud_manager.mode = -1
	vars.player_heart.visible = false
	vars.main_writer.writer_text = \"(enable:z)(sound:mono2)* Sans 'befriended your\\n mom' Undertale.\\n The highest enemy in the game.(pc)\"
	await vars.main_writer.done
	vars.hud_manager.reset()

func psiMagnet():
	vars.dialouge_manager.dirtyDict.append(vars.attack_manager.turn_num)
	vars.hud_manager.mode = -1
	vars.player_heart.visible = false
	vars.main_writer.writer_text = \"(enable:z)(sound:mono2)* You attempt to drain Sans' PP...(pc)\"
	await get_tree().create_timer(0.6).timeout
	vars.scene.get_node(\"Videos/bladeeArrow\").visible = true
	vars.scene.get_node(\"Videos/bladee\").play()
	await vars.main_writer.done
	vars.scene.get_node(\"Videos/bladeeArrow\").visible = false
	speech_bubble.visible = true
	speech_writer.writer_text = \"(Sound:Sans)We aint that close lil bro(pc)\"
	await speech_writer.done
	speech_bubble.visible = false
	vars.player_heart.visible = false
	vars.main_writer.writer_text = \"(enable:z)(sound:mono2)* Damn, rejected(pc)\"
	await vars.main_writer.done
	vars.hud_manager.reset()
"

